<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dutyroster.MainController">
  <children>
        <MenuBar>
          <menus>
            <Menu text="_File">
              <items>
                <MenuItem fx:id="menu_fileClose" onAction="#exitProgram" text="_Close">

                </MenuItem>
              </items>
            </Menu>
            <Menu text="_Tools">
              <items>
                <MenuItem fx:id="menu_nameEditor" onAction="#openEmployeeEditor" text="_Employees" />
                <MenuItem fx:id="menu_rankEditor" onAction="#openRankEditor" text="_Ranks" />
              </items>
            </Menu>
            <Menu text="_Help">
              <items>
                <MenuItem text="_About">
                </MenuItem>
              </items>
            </Menu>
          </menus>
        </MenuBar>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button mnemonicParsing="false" text="Change Order" />
         </buttons>
      </ButtonBar>
      <TabPane prefHeight="44.0" prefWidth="1312.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Roster 1">
            <content>
              <AnchorPane>
                     <children>
                        <VBox prefHeight="161.0" prefWidth="640.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="New Roster">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <ButtonBar buttonMinWidth="50.0" minHeight="50.0" prefHeight="50.0">
        <buttons>
            <Button maxHeight="31.0" maxWidth="20.0" minHeight="-Infinity" minWidth="50.0" mnemonicParsing="false" onAction="#goDown" prefHeight="31.0" prefWidth="20.0" text="&lt;&lt;" />
            <ComboBox fx:id="comboMonth" maxWidth="200.0" minWidth="200.0" onAction="#newDate" prefHeight="31.0" prefWidth="200.0" />
            <ComboBox fx:id="comboYear" layoutX="415.0" layoutY="20.0" maxWidth="120.0" minWidth="-Infinity" onAction="#newDate" prefHeight="31.0" prefWidth="120.0" />
          <Button maxHeight="-Infinity" maxWidth="20.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#goUp" prefHeight="31.0" prefWidth="20.0" text="&gt;&gt;" />
        </buttons>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </ButtonBar>
      <ScrollPane>
         <content>
            <TableView fx:id="tableView" style="-fx-background-color: transparent;">
              <columns>
                <TableColumn prefWidth="75.0" text="Employee">
                     <columns>
                      <TableColumn prefWidth="75.0" text="Rank" />
                        <TableColumn prefWidth="75.0" text="Name" />
                     </columns>
                  </TableColumn>
                  <TableColumn prefWidth="75.0" text="Increment">
                     <columns>
                        <TableColumn prefWidth="34.0" text="N" />
                        <TableColumn prefWidth="29.0" text="W" />
                        <TableColumn prefWidth="41.0" text="H" />
                     </columns>
                  </TableColumn>
              </columns>
            </TableView>
         </content>
      </ScrollPane>
  </children>
</VBox>
