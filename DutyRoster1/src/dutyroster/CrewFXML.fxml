                  <PropertyValueFactory property="rank" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="name" maxWidth="291.0" minWidth="-1.0" prefWidth="291.0" resizable="false" sortable="false" text="Name">
