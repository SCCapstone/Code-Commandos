      </TableView>
      <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="457.0">
         <children>
            <Label fx:id="ratio" prefHeight="17.0" prefWidth="420.0" />
            <Button fx:id="btnDone" mnemonicParsing="false" onAction="#closeScene" text="Done" />
