                                    <VBox minWidth="-Infinity" prefWidth="672.0" spacing="10.0" style="-fx-background-color: ffffff;">
                                       <children>
                                          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Grade and Employees" wrappingWidth="267.4700107574463">
