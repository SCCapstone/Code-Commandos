                                 <content>
                                    <VBox minWidth="-Infinity" prefHeight="846.0" prefWidth="672.0" spacing="10.0" style="-fx-background-color: ffffff;">
                                       <children>
                                          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Rosters" wrappingWidth="172.80336952209473">
