<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="483.0" prefWidth="450.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dutyroster.BlockoutController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <TableView fx:id="tableView" prefHeight="406.0" prefWidth="334.0" GridPane.rowIndex="1">
        
        <columns>
               
 
          <TableColumn fx:id="name" maxWidth="170.0" minWidth="170.0" prefWidth="170.0" resizable="false" sortable="true" text="Name">
                <cellValueFactory><PropertyValueFactory property="name" />
                </cellValueFactory>
          
          </TableColumn>
          <TableColumn fx:id="status" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" resizable="false" sortable="true" text="Status">
                <cellValueFactory><PropertyValueFactory property="status" />
                </cellValueFactory>
          </TableColumn>
         <TableColumn fx:id="fromDate" maxWidth="100.0" minWidth="80.0" prefWidth="23.7" resizable="false" sortable="true" text="From Date">
          <cellValueFactory><PropertyValueFactory property="fromDate" />
                </cellValueFactory>   
         </TableColumn>    
         <TableColumn fx:id="toDate" maxWidth="100.0" minWidth="80.0" prefWidth="67.0" resizable="false" sortable="true" text="To Date">
       <cellValueFactory><PropertyValueFactory property="toDate" />
                </cellValueFactory>   
         </TableColumn>
     </columns>    
  
    </TableView>
   
   <HBox alignment="bottom_right" spacing="10.0" style="-fx-alignment: center;" GridPane.rowIndex="2">
      <ComboBox fx:id="statusCombo" prefHeight="31.0" prefWidth="145.0" promptText="Status" />
          <ComboBox fx:id="nameCombo" layoutX="40.0" prefHeight="31.0" prefWidth="142.0" promptText="Name" />
     </HBox>
   <HBox spacing="20.0" style="-fx-alignment: center;" GridPane.rowIndex="3">
      <children>
         <DatePicker fx:id="dateFrom" prefWidth="120.0" promptText="From" />
         <DatePicker fx:id="dateTo" prefWidth="120.0" promptText="To" />
      </children>
   </HBox>
   <HBox alignment="bottom_right" layoutX="20.0" layoutY="437.0" spacing="10" style="-fx-alignment: center;" GridPane.rowIndex="4">
      <children>
             <Button maxWidth="420.0" onAction="#addBlockout" prefWidth="420.0" text="Add" />
      </children>
   </HBox>


   
</VBox>
