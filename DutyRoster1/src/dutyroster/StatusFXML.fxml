<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="CENTER" hgap="5.0" prefHeight="483.0" prefWidth="354.0" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dutyroster.StatusController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
    </padding>
    <TableView fx:id="tableView" prefHeight="406.0" prefWidth="334.0" GridPane.rowIndex="1">
        <columns>
               
            <TableColumn fx:id="sort" editable="false" minWidth="-Infinity" prefWidth="97.0" resizable="false" sortable="true" text="Sort" visible="false">
                <cellValueFactory><PropertyValueFactory property="sort" />
                </cellValueFactory>
          </TableColumn>
          <TableColumn fx:id="status" minWidth="-Infinity" prefWidth="103.0" resizable="false" sortable="false" text="Code">
                <cellValueFactory><PropertyValueFactory property="status" />
                </cellValueFactory>
          
          </TableColumn>
          <TableColumn fx:id="name" maxWidth="245.0" minWidth="-1.0" prefWidth="226.0" resizable="false" sortable="false" text="Title">
                <cellValueFactory><PropertyValueFactory property="name" />
                </cellValueFactory>
          </TableColumn>

     </columns>    
  
    </TableView>
   
   <HBox alignment="bottom_right" prefHeight="309.0" prefWidth="334.0" spacing="10" GridPane.rowIndex="2">
          <TextField fx:id="statusField" prefHeight="31.0" prefWidth="46.0" promptText="Code" />
      <TextField fx:id="statusField2" prefHeight="31.0" prefWidth="125.0" promptText="Title" />
          <Button onAction="#addStatus" text="Add" />
     </HBox>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
      <RowConstraints maxHeight="724.0" minHeight="143.0" prefHeight="409.0" />
      <RowConstraints maxHeight="617.0" minHeight="36.0" prefHeight="44.0" />
   </rowConstraints>
   
</GridPane>
